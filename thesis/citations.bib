@misc{zhangMethodArchitectureMedical2025,
  title = {A {{Method}} for the {{Architecture}} of a {{Medical Vertical Large Language Model Based}} on {{Deepseek R1}}},
  author = {Zhang, Mingda and Qin, Jianglong},
